<%= form_for(@shipment_type, url: { action: action}, html: { multipart: true } ) do |f| %>
     Name <%= f.text_field :name %>
     Description <%= f.text_field :description %>
     Order Index <%= f.number_field :order_index %>
     Picture <%= f.file_field :picture %>
     <%= f.submit(action.to_s.capitalize) %>
<% end %>
